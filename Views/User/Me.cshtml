@model where_we_go.DTO.UserResponseDto
@{
    ViewData["Title"] = "My Profile";
}

<h1>My Profile</h1>
<div>
    <p>Email: @Model.Email</p>
    <p>Username: @Model.UserName</p>
    <ul>
        @foreach (var role in @Model.Role ?? Array.Empty<string>())
        {
            <li>@role</li>
        }
    </ul>
</div>